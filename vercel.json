{
  "version": 2,
  "builds": [
    {
      "src": "api/files.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/apifiles.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/apistats.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/apidownload[code].js",
      "use": "@vercel/node"
    },
    {
      "src": "api/apisave.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/files",
      "dest": "/api/files.js"
    },
    {
      "src": "/api/stats",
      "dest": "/api/apistats.js"
    },
    {
      "src": "/api/save",
      "dest": "/api/apisave.js"
    },
    {
      "src": "/api/download",
      "dest": "/api/apifiles.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/mobile",
      "dest": "/mobile.html"
    }
  ]
}

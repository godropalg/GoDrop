{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/files",
      "dest": "/api/getfiles.js"
    },
    {
      "src": "/api/download/:code",
      "dest": "/api/download.js?code=$1"
    },
    {
      "src": "/api/save",
      "dest": "/api/savefile.js"
    },
    {
      "src": "/api/stats",
      "dest": "/api/apistats.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}
